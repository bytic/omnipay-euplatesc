{
  "name": "bytic/omnipay-euplatesc",
  "type": "library",
  "license": "MIT",
  "description": "Euplatesc driver for ThePhpLeague Omnipay multi-payment processing library",
  "keywords": [
    "euplatesc",
    "driver",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/ByTIC/omnipay-euplatesc",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/bytic/omnipay-tests",
      "no-api": true
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Omnipay\\Euplatesc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Omnipay\\Euplatesc\\Tests\\Fixtures\\": "tests/fixtures",
      "ByTIC\\Omnipay\\Euplatesc\\Tests\\": "tests/src"
    }
  },
  "require": {
    "php": "^7.0|^8.0",
    "bytic/omnipay-common": "^2.0"
  },
  "require-dev": {
    "omnipay/tests": "^2.0",
    "vlucas/phpdotenv": "^2.3"
  }
}
